public class WorldEnvironment {
  private static WorldEnvironment instance;

  public String environment
  public String enemy

  public WorldEnvironment(String environement){
    this.environment = environment;
    this.enemy = EnemyFactory.Enemy;
}
  public static WorldEnvironment getInstance(String environment){
    if(instance==null){
      return new WorldEnvironment(environment);
    }
    return instance;
}
    public void setEnvironment(String environment) {
        this.environment = environment;
        this.enemy = EnemyFactory.createEnemy;
    }
    public String getEnvironment() {
        return environment;
    }
}
